      "source": [
        "import random\n",
        "\n",
        "var = random.randint(10,15)\n",
        "\n",
        "print(\"ランダムな数値は\" + str(var))\n",
        "\n",
        "if var % 15 == 0:\n",
        "    print(\"FizzBuzz\" + \"3かつ5の倍数です\")\n",
        "\n",
        "elif var % 3 == 0:\n",
        "    print(\"Fizz\" + \"3の倍数です\")\n",
        "\n",
        "elif var % 5 == 0:\n",
        "    print(\"Buzz\" + \"5の倍数です\")\n",
        "\n",
        "else:\n",
        "    print(str(var) + \"は、上記のどの場合にも該当しない\")\n",
        "\n",
        "\n"
      ]
